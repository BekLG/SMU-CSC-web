<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
  <script src="https://kit.fontawesome.com/yourcode.js" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="css/style.css">
  <link rel="stylesheet" href="css/newsEvents.css">
  <link href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
  <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <title>Events</title>
</head>
<body>
  <main>
    <div class="big-wrapper light">
      <img src="images/shape.png" alt="" class="shape" />

      <header class="fixed-top">
        <div class="container fixed-top">
          <div class="logo">
            <h3>SMU-CSC</h3>
          </div>

          <div class="links">
            <ul>
              <li><a href="/">Home</a></li>
              <li><a href="/news">News</a></li>
              <li><a href="/events">Events</a></li>
              <li><a href="/gallery">Gallery</a></li>
              <li><a href="/termsAndConditions" class="btn">Register</a></li>
            </ul>
          </div>

          <div class="overlay"></div>

          <div class="hamburger-menu">
            <div class="bar"></div>
          </div>
        </div>
      </header>

          
            <div class="big-title-new" style="padding-top: 5rem;">
              <h1>Events</h1>
            </div>
            <section class="events-section">

              <div class="eventscard-container">
              
                <% Events.forEach(function(event){ %>

                  <div class="eventscard eventscard-blue">
                    <div class="eventscard-innerbox">
                        <!--<img class="eventscard-img2" src="<%=event.imageURL%>" />-->
                        <img class="eventscard-img" src="<%=event.imageURL%>" />
                        <div class="eventscard-textbox">
                            <div class="eventscard-title"><%=event.title%></div>
                            <!-- <div class="eventscard-subtitle">Event </div> -->
                            <div class="eventscard-bar"></div> <%=event.description%>

                            <%if(event.registrationRequired === true){%>
                              <div class="eventscard-description"> 
                                <p>Register <a href="<%=event.registrationLink%>"> Here</a></p>
                              </div>
                            <% } %>

                            
                            <br>
                            <div class="eventscard-tagbox">
                                <span class="eventscard-tag"><i class="fa fa-calendar-o" aria-hidden="true"></i> <%=event.dateAndTime.toDateString()%> </span>

                                <% var hours = event.dateAndTime.getHours(); %>
                                <% var meridiem = hours >= 12 ? 'PM' : 'AM'; %>
                                <% hours = hours % 12 || 12; // Convert to 12-hour format %>
                                <% var minutes = event.dateAndTime.getMinutes(); %>
                                <% var formattedTime = hours + ':' + minutes + ' ' + meridiem; %>

                                <span class="eventscard-tag"><i class="fa fa-clock-o" aria-hidden="true"></i> <%=formattedTime %> </span>
                                <span class="eventscard-tag"><i class="fa fa-map-marker" aria-hidden="true"></i> <%=event.venue%></span>
                            </div>
                        </div>
                       

                        <%if(event.registrationRequired=== true){%>

                          <div class="posted-date">
                            <p>Registration Deadline <%=event.registrationDeadline.toDateString()%></p>
                          </div>

                          <% } %>
                        
                    </div>
                </div>

                <% }) %>
                  
                  
                  
                 
                  
                  
              </div>
              </section>
          
      <div class="bottom-area">
        <div class="container">
          <button class="toggle-btn mode">
            <i class="far fa-moon"></i>
            <i class="far fa-sun"></i>
          </button>
        </div>
      </div>

    </div>
  </main>
        
    <section id="footer">
        <div class="container">
          <div class="row text-center text-xs-center text-sm-left text-md-left">
            <div class="col-xs-12 col-sm-4 col-md-4">
              <h5>Quick links</h5>
              <ul class="list-unstyled quick-links">
                <li><a href="#"><i class="fa fa-angle-double-right" aria-hidden="true"></i>Home</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i>About</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i>FAQ</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i>Get Started</a></li>
                
              </ul>
            </div>
            <div class="col-xs-12 col-sm-4 col-md-4">
              <h5>Quick links</h5>
              <ul class="list-unstyled quick-links">
                <li><a href="#"><i class="fa fa-angle-double-right"></i>Home</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i>About</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i>FAQ</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i>Get Started</a></li>
                
              </ul>
            </div>
            <div class="col-xs-12 col-sm-4 col-md-4">
              <h5>Quick links</h5>
              <ul class="list-unstyled quick-links">
                <li><a href="#"><i class="fa fa-angle-double-right"></i>Home</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i>About</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i>FAQ</a></li>
                <li><a href="#"><i class="fa fa-angle-double-right"></i>Get Started</a></li>
                
              </ul>
            </div>
          </div>
          <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 mt-2 mt-sm-5">
              <ul class="list-unstyled list-inline social text-center">
                <li class="list-inline-item"><a href="#"><i class="fa fa-facebook"></i></a></li>
                <li class="list-inline-item"><a href="#"><i class="fa fa-twitter"></i></a></li>
                <li class="list-inline-item"><a href="#"><i class="fa fa-instagram"></i></a></li>
                <li class="list-inline-item"><a href="#"><i class="fa fa-google-plus"></i></a></li>
                <li class="list-inline-item"><a href="#" target="_blank"><i class="fa fa-envelope"></i></a></li>
              </ul>
            </div>
            </hr>
          </div>  
          <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 mt-2 mt-sm-2 text-center text-white">
              <p><u>St. Mary's Computer Science Club</u></p>
              <p class="h6">&copy All right Reserved.</p>
            </div>
            </hr>
          </div>  
        </div>
     </section>
     
<script src="js/app.js"></script> 
</body>
</html>


